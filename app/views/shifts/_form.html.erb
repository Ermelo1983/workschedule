<%= simple_form_for @shift do |f| %>
<%#devise_error_messages!%>
<%= f.input :name %>
<%= f.input :description %>
<%= f.input :start_time %>
<%= f.input :end_time %>
<%= f.input :quantity_per_day %>
<%= f.input :for_location %>
<%= f.input :for_unit %>

<div class="actions">
  <%= f.submit "Save" %>
</div>
<% end %>
